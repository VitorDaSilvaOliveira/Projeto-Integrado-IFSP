    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.css">

    <link rel="stylesheet" href="~/css/home.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/css/estoque/theme.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/css/estoque/theme/plugins/prism.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/css/estoque.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/css/jjmasterdata.css" asp-append-version="true"/>

